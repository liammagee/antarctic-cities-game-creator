{
  "name": "antartctic-futures",
  "version": "1.0.0",
  "description": "Antarctic Futures is a Plague-inspired game built on Cocos Creator, for simulating climate change and policies to address it.",
  "main": "index.js",
  "dependencies": {
    "@maxmind/geoip2-node": "^1.4.0",
    "@types/node": "^14.0.12",
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "express": "^4.17.1",
    "geoip-lite": "^1.4.2",
    "iplocation": "^7.2.0",
    "maxmind": "^4.1.3",
    "simple-statistics": "^7.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --colors",
    "basic": "$(which http-server) -o -p 8000 ./build/web-mobile",
